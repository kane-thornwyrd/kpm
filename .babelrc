{
  "presets": [
    ["env", {
      "targets": {
        "node": ["8.9.4"]
      }
    }],
    "react"
  ],
  "plugins": [
    "transform-class-properties",
    "transform-object-rest-spread",
    "transform-react-jsx",
    ["transform-inline-environment-variables", {
      "include": [
        "NODE_ENV",
        "KPR_ENV"
      ]
    }]
  ]
}
